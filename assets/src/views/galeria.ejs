<%- include('./components/gallhead') %>
<body class="flex">
    <div class="flex flex-col">
    <%- include('./components/navbar') %>
        <div>
            <%- include('./components/gallnav') %>
        </div>
        <div class="flex w-full h-screen bg-white flex-wrap overflow-y-auto">
            <div class="w-full h-10"></div>
            <% images.forEach(function(image){ %>
            <div class="flex flex-col ml-28 mt-28 bg-gray-200 h-3/6 w-1/4 shadow-2xl">
                <div class="h-5/6 w-full bg-gray-500 ">
                    <img class="object-cover h-full w-full" src="<%= image.pic %>">
                </div>
                <p class="m-14 mt-12 font-bold text-lg overflow-ellipsis"><%= image.desc %></p>
            </div>
            <% }) %>
        </div>
    <%- include('./components/footer') %>
</body>

</html>